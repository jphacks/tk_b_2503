{
  "name": "jphacks",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "setup": "corepack enable pnpm",
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "pnpm run eslint && pnpm run prettier && pnpm run stylelint",
    "lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix && pnpm run stylelint:fix",
    "eslint": "eslint",
    "eslint:fix": "eslint --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "stylelint": "stylelint \"src/**/*.css\"",
    "stylelint:fix": "stylelint \"src/**/*.css\" --fix",
    "typecheck": "npx tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "gen": "cmk",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.908.0",
    "@aws-sdk/s3-request-presigner": "^3.908.0",
    "@libsql/client": "^0.15.15",
    "@material-symbols/svg-400": "^0.38.0",
    "@t3-oss/env-core": "^0.13.8",
    "@t3-oss/env-nextjs": "^0.13.8",
    "better-auth": "^1.3.27",
    "drizzle-orm": "^0.44.6",
    "jotai": "^2.15.0",
    "nanoid": "^5.1.6",
    "next": "15.5.5",
    "next-safe-action": "^8.0.11",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-easy-crop": "^5.5.3",
    "server-only": "^0.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@css-modules-kit/codegen": "0.6.0",
    "@css-modules-kit/stylelint-plugin": "0.4.0",
    "@eslint/js": "^9.37.0",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/nextjs-vite": "^9.1.10",
    "@types/node": "^24.7.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prefer-arrow-functions": "^3.9.1",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-storybook": "^9.1.10",
    "knip": "^5.65.0",
    "lefthook": "1.13.6",
    "prettier": "^3.6.2",
    "storybook": "^9.1.10",
    "stylelint": "16.25.0",
    "stylelint-config-recess-order": "7.3.0",
    "stylelint-config-standard": "39.0.1",
    "stylelint-plugin-use-baseline": "1.0.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "packageManager": "pnpm@10.18.3"
}
